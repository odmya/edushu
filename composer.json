{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "backpack/backupmanager": "^2.0",
        "backpack/crud": "4.1.x-dev as 4.0.99",
        "backpack/filemanager": "^1.0",
        "backpack/logmanager": "^3.0",
        "backpack/menucrud": "^2.0",
        "backpack/newscrud": "^3.0",
        "backpack/pagemanager": "^2.0",
        "backpack/permissionmanager": "^5.0",
        "backpack/revise-operation": "^1.0",
        "backpack/settings": "^3.0",
        "barryvdh/laravel-elfinder": "^0.4.5",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.3",
        "laravel/framework": "^6.2",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^2.0",
        "overtrue/laravel-lang": "^3.0",
        "overtrue/laravel-wechat": "~5.0",
        "overtrue/wechat": "^4.2",
        "spatie/laravel-backup": "^6.1",
        "spatie/laravel-translatable": "^4.2",
        "webfactor/laravel-backpack-instant-fields": "^3.0"
    },
    "require-dev": {
        "backpack/generators": "^2.0",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^2.0",
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
